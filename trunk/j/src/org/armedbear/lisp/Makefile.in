SOURCEFILES = $(patsubst %.class,%.java,$(CLASSFILES))

%.class : %.java
	$(JAVAC) -classpath "$(COMPILER_CLASSPATH)" $(JAVACFLAGS) $<
	date > build
	touch ../../../../classes.stamp

classes: $(CLASSFILES)

lisp: $(CLASSFILES)
	gcj -O2 $(SOURCEFILES) --main=org.armedbear.lisp.Main -o lisp

clean:
	-rm -f *.class build

Makefile: Makefile.in ../../../../config.status
	cd ../../../.. && $(SHELL) ./config.status
