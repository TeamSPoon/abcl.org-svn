%.class : %.java
	$(JAVAC) -classpath "$(COMPILER_CLASSPATH)" $(JAVACFLAGS) $<
	date > build
	touch ../../../../classes.stamp

all: $(CLASSFILES)

clean:
	-rm -f *.class build

Makefile: Makefile.in ../../../../config.status
	cd ../../../.. && $(SHELL) ./config.status

